{
  "AWSEBDockerrunVersion": "1",
  "Image": {
    "Name": "your-account.dkr.ecr.region.amazonaws.com/fastapi-tutor:latest",
    "Update": "true"
  },
  "Ports": [
    {
      "ContainerPort": "8000",
      "HostPort": "8000"
    }
  ],
  "Volumes": [
    {
      "HostDirectory": "/var/app/uploads",
      "ContainerDirectory": "/app/uploads"
    }
  ],
  "Logging": "/var/log/nginx"
} 